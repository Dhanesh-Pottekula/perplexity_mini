{
  "name": "perplexity",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && npm run fix-imports",
    "fix-imports": "node fix-imports.js",
    "start": "node dist/server.js",
    "start:debug": "node --inspect-brk dist/server.js",
    "dev": "tsx server.ts",
    "dev:watch": "nodemon --inspect --exec \"tsx\" server.ts",
    "start:pm2": "pm2 start ecosystem.config.cjs",
    "pm2:reload": "pm2 reload perplexity",
    "pm2:restart": "pm2 restart perplexity",
    "pm2:stop": "pm2 stop perplexity",
    "pm2:delete": "pm2 delete perplexity",
    "pm2:logs": "pm2 logs perplexity",
    "pm2:list": "pm2 ls",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@qdrant/js-client-rest": "^1.15.1",
    "body-parser": "^2.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.7.0",
    "mongoose": "^8.6.1",
    "playwright": "^1.55.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^5.0.3",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^24.5.2",
    "esbuild": "^0.25.10",
    "nodemon": "^3.1.10",
    "pm2": "^5.4.3",
